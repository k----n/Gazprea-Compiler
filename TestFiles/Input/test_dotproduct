procedure main() returns integer {

    integer vector i = 1..3 by 1;

    integer vector v = 2..4 by 1;
    real vector x = [1.5, 2.3];

    real vector y = [6.9, 2.7];

    real vector z = [2.2 , 3.0, 4.0];

    var out = std_output();

    (i ** v) -> out;



    (x ** y) -> out;

    (z ** v) -> out;

    return 0;
}
