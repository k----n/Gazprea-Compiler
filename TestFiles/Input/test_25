procedure main() returns integer{

integer x = 1; 
integer y = 10;
var out = std_output(); 

/* Since the conditional is tested after the execution '10' is printed */ 
loop {
x -> out; 
'\n' -> out;
x = x + 1;

loop {
y -> out; 
'\n' -> out;
y = y + 1;



}while y < 12;

}while x < 5;

	return 0;
}
