procedure main() returns integer {

    integer interval i = 1..10;

    integer interval v = 2..9;

    integer interval j = i + v;
    integer interval k = i * v;

    integer interval l = i / v;

    integer interval m = +i;

    integer interval n = -i;

    var out = std_output();

    i == v -> out;

    i != v -> out;

    i == i -> out;

    return 0;
}
