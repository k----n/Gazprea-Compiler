procedure printint(const integer x) {
    x -> std_output();
}

procedure printtup(tuple(character, integer, real) t) {
    t.1 -> std_output();
    t.2 -> std_output();
    t.3 -> std_output();
}

procedure main() returns integer {
    var tup = ('i', 1, 1.2);
    tuple(character, integer, real) tup2 = ('j', 2, 2.4);

    tup.1 -> std_output();
    tup.2 -> std_output();
    tup.3 -> std_output();

    integer x = 3;
    call printint(tup.2);
    call printint(x);
    call printtup(tup);
    call printtup(tup2);

    return 0;	  
}