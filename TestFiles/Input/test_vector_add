procedure main() returns integer{

    ([1,2,3] + 1) -> std_output();
    '\n' -> std_output();
    (1 + [1,2,3]) -> std_output();
    '\n' -> std_output();
    ([1,1,1] + [1,2,3]) -> std_output();
    '\n' -> std_output();

    ([1,2,3] + 1.2) -> std_output();
    '\n' -> std_output();
    (1.2 + [1,2,3]) -> std_output();
    '\n' -> std_output();
    ([1.2,1.2,1.2] + [1,2,3]) -> std_output();
    '\n' -> std_output();

return 0;
}